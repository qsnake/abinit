    echo "        +--------------------------------------------------------------+"
  echo ""

  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: message file ${abi_msg_file} not found" >&5
$as_echo "$as_me: WARNING: message file ${abi_msg_file} not found" >&2;}
  fi

