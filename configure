$as_echo "$as_me: fcflags_opt_etsf_io overriden by FCFLAGS" >&6;}
    fi
  fi


    if test "${FCFLAGS}" = ""; then
    if test "${fcflags_opt_fox}" = "" -o "${enable_optim}" = "no"; then
