    fi
  fi


    if test "${FCFLAGS}" = ""; then
    if test "${fcflags_opt_etsf_io}" = "" -o "${enable_optim}" = "no"; then
      fcflags_opt_etsf_io="${fcflags_opt_default}"
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: optimization for etsf_io is ${fcflags_opt_etsf_io}" >&5
